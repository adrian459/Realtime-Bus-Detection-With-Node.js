    <div class="container mt-3 bg-white">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">No</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Password</th>
          </tr>
        </thead>
        <tbody>
          <% user.forEach(users => { %>
            <tr>
              <th scope="row">1</th>
              <td><%= users.nama %> </td>
              <td><%= users.email %> </td>
              <td><%= users.password %> </td>
            </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
    
